{
  "family": "pexelhub",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::266735814394:role/PH-BASE-STK-ECSExecutionRole-Wz6urxcvzA8p",
  "taskRoleArn": "arn:aws:iam::266735814394:role/PH-BASE-STK-ECSTaskRole-oBITGsp1ly1l",
  "containerDefinitions": [
    {
      "name": "pexelhub-app-container",
      "image": "266735814394.dkr.ecr.eu-central-1.amazonaws.com/pexelhub-ecr:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/actuator/health || exit 1"
        ],
        "interval": 30,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pexelhub",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "DB_HOST",
          "value": "ph-base-stk-rdsinstance-iitcr6zm8bhv.cxii6mikg9c3.eu-central-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "AWS_S3_BUCKET",
          "value": "pexelhub-bkt"
        }
      ],
      "secrets": [
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:266735814394:secret:rds-master-secret-63zJQK:username::"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:266735814394:secret:rds-master-secret-63zJQK:password::"
        }
      ]
    }
  ]
}